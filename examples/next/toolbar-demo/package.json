{
  "name": "hwptoolkit-example-nextjs-toolbar-demo",
  "version": "1.0.0",
  "description": "React hooks toolbar demo with Next.js App Router",
  "private": true,
  "scripts": {
    "example:dev": "npm --prefix ./example-app run dev",
    "example:build": "npm --prefix ./example-app run build",
    "example:start": "node scripts/setup-env.js && concurrently \"npm run wp:start\" \"npm run example:dev\"",
    "example:stop": "npm run wp:stop",
    "wp:start": "npm install && wp-env start",
    "wp:stop": "wp-env stop",
    "wp:destroy": "wp-env destroy"
  },
  "keywords": [
    "headless",
    "wordpress",
    "nextjs",
    "toolbar",
    "react-hooks",
    "wpgraphql"
  ],
  "author": "WP Engine",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@wordpress/env": "^10.31.0",
    "concurrently": "^8.2.2"
  }
}
